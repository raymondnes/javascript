// Array elements are arranged by index values, starting at
// 0 as the first element index. Elements can be
// accessed by their index using the array name, and the
// index surrounded by square brackets.


// Accessing an array element
   const myArray = ["apple", "Orange", "Pineapple", "Potato"];
   console.log(myArray[2]); // pineapple
   console.log(myArray[0]); // apple
   console.log(myArray[3]); // potato
